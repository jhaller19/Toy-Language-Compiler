class IteratorTest{

type
    MyArray = array integer[5];

VAR
    MyArray ar;
    Iterator(ar) it;
    integer i;



procedure main(){
    {
        ar[0] = 0;
        ar[1] = 1;
        ar[2] = 2;
        ar[3] = 3;
        ar[4] = 4;
        while(it.hasNext()){
             {
             i = it.next();
             printf('%d',i);
             }
        }

    }
}
}